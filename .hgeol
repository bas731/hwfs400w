[patterns]
{.hgeol,.hgignore,.hgtags} = native
**.cmd = CRLF
**.sh = LF
**.{c,cpp,cc,h} = native
**.{java,properties} = native
**.{xml,xsl,dtd,ent} = native
**.{html,css,js,txt} = native
**.g = native
**/Makefile = LF
