<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<display-name>Scan Services</display-name>

<!--
	<listener>
		<listener-class>StartUpListener</listener-class>
	</listener>
-->

	<servlet>
		<description></description>
		<display-name>S400W Scan Service</display-name>
		<servlet-name>s400w</servlet-name>
		<servlet-class>hwfs400w.S400WServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>s400w</servlet-name>
		<url-pattern>/s400w/*</url-pattern>
	</servlet-mapping>


